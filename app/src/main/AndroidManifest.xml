<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.salinas.britocajero">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".dni"
            android:label="@string/title_activity_dnin"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".principal"
            android:label="@string/title_activity_principal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".deposito"
            android:label="@string/title_activity_deposito"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".pin"
            android:label="@string/title_activity_pin"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".saldo"
            android:label="@string/title_activity_saldo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".retiro"
            android:label="@string/title_activity_retiro"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>